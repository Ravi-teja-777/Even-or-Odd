while True:
    n=int(input("Enter a how numbers to check : "))
    a=[]
    while n-1>=0:
        i=int(input("Enter a number : "))
        if i%2==0:
            print(i,"is an Even Number")
        else :
            print(i,"is a Odd Number")
        a.append(i)
        n=n-1
    print(a)
    
    x=int(input("enter a number to check : "))
    if x in a:
        print(x,"is present in the list ")
    else:
        print(x,"is not present in the lsit")
